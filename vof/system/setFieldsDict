FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.metal 0
);

regions
(
    sphereToCell
    {
        centre (16.25e-6 0 0);
        radius 16.25e-6;
        fieldValues
        (
            volScalarFieldValue alpha.metal 1
        );
    }

    sphereToCell
    {
        centre (-16.25e-6 0 0);
        radius 16.25e-6;
        fieldValues
        (
            volScalarFieldValue alpha.metal 1
        );
    }
);

