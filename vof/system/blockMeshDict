vertices
(
    (-48.75 -48.75 -48.75)  //0 (x, y, z)
    (48.75 -48.75 -48.75)   //1
    (48.75 48.75 -48.75)    //2
    (-48.75 48.75 -48.75)   //3
    (-48.75 -48.75 48.75)   //4
    (48.75 -48.75 48.75)    //5
    (48.75 48.75 48.75)     //6
    (-48.75 48.75 48.75)    //7
);
